package components

templ HomePage() {
    @PageBuilder(
        Nav(),
        Home(),
    )
}

templ Home (){
    <div class="home_container">
    <section class="section">
         <div class="first_section_content">
            <h1 class="title">Solana Store</h1>
            <p>Welcome to Solana Store, your premier destination for seamless shopping using Solana cryptocurrency.
             Our store offers a wide range of products, from cutting-edge electronics to trendy fashion items, all available for purchase with the fast and secure Solana blockchain.
             </p>
           <a><button class="button-85">EXPLORE PRODUCTS</button></a>
